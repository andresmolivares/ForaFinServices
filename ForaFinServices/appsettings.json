{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SecApiSettings": {
    "BaseUrl": "https://data.sec.gov/api/xbrl/companyfacts/",
    "UserAgent": "PostmanRuntime/7.34.0",
    "Accept": "*/*"
  },
  "ParallelSettings": {
    "MaxDegreeOfParallelism": 6
  },
  "BatchSettings": {
    "Size": 20
  },
  "CacheRefreshSettings": {
    "RefreshIntervalInMinutes": 5,
    "AbsoluteExpirationInMinutes": 20,
    "SlidingExpirationInMinutes": 10
  },
  "CikSettings": {
    "FileName": "Samples/CIKs.csv"
  },
  "RetryPolicySettings": {
    "RetryAfterSeconds": 5,
    "RetryCount":  10
  }
}
